#maximal read length
max_rd_len=150

[LIB]
#if sequence needs to be reversed
reverse_seq=0
#in which part(s) the reads are used
asm_flags=1
#use only first 85 bps of each read works fine
rd_len_cutoff=120
#in which order the reads are used while scaffolding
rank=1
# cutoff of pair number for a reliable connection (default 3)
pair_num_cutoff=3
#minimum aligned length to contigs for a reliable read location (default 32)
map_len=45
#fastq file for read 1
q=tmp/cons/R1cons.reads.fq
